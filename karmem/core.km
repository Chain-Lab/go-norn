karmem structs @packed(true) @golang.package(`core`);

struct TransactionBody table {
    Hash [32]byte;
    Address [20]byte;
    Receiver [20]byte;
    Gas int64;
    Nonce int64;
    Event []byte;
    Opt []byte;
    State []byte;
    Data []byte;
    Expire int64;
    Height int64;
    Index int64;
    BlockHash [32]byte;
    Timestamp int64;
    Public [33]byte;
    Signature [<73]byte;
    IP [4]byte;
}

struct Transaction inline {
    Body TransactionBody;
}

struct GenesisParams table {
    Order [128]byte;
    TimeParam int64;
    Seed [32]byte;
    VerifyParam [32]byte;
}

struct GeneralParams table {
    Result []byte;
    Proof []byte;
    RandomNumber [33]byte;
    S []byte;
    T []byte;
}

struct BlockHeader table {
    Timestamp int64;
    PrevBlockHash [32]byte;
    BlockHash [32]byte;
    MerkleRoot [32]byte;
    Height int64;
    PublicKey [33]byte;
    Params []byte;
    GasLimit int64;
    IPs []byte;
}

struct Block table {
    Header BlockHeader;
    Transactions []Transaction;

}

struct DataCommand table {
    Opt []byte;
    Key []byte;
    Value []byte;
}